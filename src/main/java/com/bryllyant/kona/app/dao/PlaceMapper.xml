    </if>

    <if test="endDate != null">
      and created_date &lt; #{endDate,jdbcType=TIMESTAMP}
    </if>

    <!-- match within radius -->
    and st_within(coords,
    st_envelope(
    linestring(point(@rlon1, @rlat1), point(@rlon2, @rlat2))
    )
    )

    order by st_distance(point(@lon, @lat), coords)
