  <update id="updatePresence" parameterType="java.lang.Integer">
    set @ONLINE = 100;
    set @OFFLINE = 999;

    <!--
        update all users whose status is null or not offline
        and who last accessed the API more than inactiveSeconds ago
    -->

    update kona__user a,
    kona__api_log b
    set a.presence_id = @OFFLINE
    where (a.presence_id is null OR a.presence_id != @OFFLINE)
    and a.id = b.user_id
    and TIMESTAMPDIFF(SECOND,b.created_date,now()) &gt; #{inactiveSeconds}
    and b.id = (
    select max(id)
    from kona__api_log
    where user_id = a.id
    );


    <!--
        update all users whose status is null or offline
        and who last accessed the API less than inactiveSeconds ago
    -->

    update kona__user a,
    kona__api_log b
    set a.presence_id = @ONLINE
    where (a.presence_id is null OR a.presence_id = @OFFLINE)
    and a.id = b.user_id
    and TIMESTAMPDIFF(SECOND,b.created_date,now()) &lt; #{inactiveSeconds}
    and b.id = (
    select max(id)
    from kona__api_log
    where user_id = a.id
    );

  </update>




</mapper>